<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="s">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:elegantTextHeight">true</item>
        <item name="android:freezesText">true</item>
        <item name="android:textAppearance">@style/TextAppearance.AppCompat.Display1</item>
        <item name="android:textStyle">normal|bold|italic</item>
    </style>

    <style name="Btnall" parent="Widget.AppCompat.Button.Borderless">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_margin">5dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:height">70dp</item>
        <item name="android:background">@drawable/circle</item>
        <item name="android:backgroundTint">#009688</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">24sp</item>
    </style>
</resources>