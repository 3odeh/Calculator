<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="study.app.mathmatical">

    <application
        android:allowBackup="true"
        android:icon="@drawable/square_btn"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Mathmatical">
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:configChanges="orientation">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".calculator"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".ObjectArea"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".Rectangle"
            android:parentActivityName=".MainActivity" />
        <activity android:name=".Triangle"
            android:parentActivityName=".MainActivity"></activity>
        <activity android:name=".Circle"
            android:parentActivityName=".MainActivity"></activity>
        <activity android:name=".Square"
            android:parentActivityName=".MainActivity"></activity>
    </application>

</manifest>